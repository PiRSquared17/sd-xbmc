<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<addons>
<addon id="plugin.moje.polskieradio"
       name="Moje Polskie Radio"
       version="0.0.1"
       provider-name="SYSTEMS DESIGN - Plesken">
  <requires>
    <import addon="xbmc.python" version="1.0"/>
    <import addon="script.module.simplejson" version="2.0.10" />
  </requires>
  <extension point="xbmc.python.pluginsource"
            library="default.py">
        <provides>audio</provides>
  </extension>
  <extension point="xbmc.addon.metadata">
    <summary>Moje Polskie Radio</summary>
    <description>Moje Polskie Radio</description>
    <platform>all</platform>
  </extension>
</addon>

<addon id="plugin.multimedia.players" name="External Multimedia Players"
	version="0.1.0" provider-name="Plesken">
	<requires>
		<import addon="xbmc.python" version="1.0" />
	</requires>
	<extension point="xbmc.python.pluginsource" library="default.py">
		<provides>video</provides>
	</extension>
	<extension point="xbmc.addon.metadata">
		<summary>External Multimedia Players</summary>
		<description>These players you can use to play 3D movies from NET and bluray from iso files, or unencrypted disc</description>
		<platform>all</platform>
	</extension>
</addon>

<addon id="plugin.video.polishtv.live"
       name="Telewizja Polska (kanaÅ‚y, VOD, filmy, seriale)"
       version="0.0.1"
       provider-name="Plesken">
  <requires>
    <import addon="xbmc.python" version="1.0"/>
    <import addon="script.module.elementtree" version="1.2.7" />
  </requires>
  <extension point="xbmc.python.pluginsource"
            library="default.py">
        <provides>video</provides>
  </extension>
  <extension point="xbmc.addon.metadata">
    <summary>Polish TV: Watch LIVE VIDEO STREAMING of International Cricket</summary>
    <description>Favorite Cricket Channel in Poland.</description>
    <platform>all</platform>
  </extension>
</addon>
</addons>
