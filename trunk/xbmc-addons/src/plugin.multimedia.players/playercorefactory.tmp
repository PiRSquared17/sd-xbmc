<playercorefactory>
 <players>
   <player name="3Dbino" type="ExternalPlayer" audio="false" video="true">
     <filename>%arg0%</filename>
     <args>"{1}"</args>
     <hidexbmc>false</hidexbmc>
     <hideconsole>false</hideconsole>
     <warpcursor>none</warpcursor>
   </player>
 </players>
 <rules action="prepend">
   <rule filetypes="mkv|wmv" filename=".*3D.*|.*3d.*" player="3Dbino"/>
 </rules>
</playercorefactory>
